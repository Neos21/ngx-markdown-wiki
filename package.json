{
  "name": "ngx-markdown-wiki",
  "description": "Markdown-based Wiki system made with Angular",
  "private": true,
  "scripts": {
    "?ng": "echo 'Angular CLI'",
    "ng": "ng",
    "?start": "echo 'Develop'",
    "start": "ng serve --open --base-href /",
    "?lint": "echo 'Lint'",
    "lint": "ng lint --type-check",
    "?build": "echo 'GitHub Pages 用にビルドする'",
    "build": "ng build --base-href ./ --prod",
    "?docs": "echo 'npm run build 後、ドキュメントをコピーする'",
    "docs": "rimraf ./docs/assets/docs/ && cpx './src/assets/docs/**/*' ./docs/assets/docs/"
  },
  "author": "Neos21 <neos21@gmail.com> (http://neo.s21.xrea.com/)",
  "license": "MIT",
  "homepage": "https://github.com/Neos21/ngx-markdown-wiki#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neos21/ngx-markdown-wiki.git"
  },
  "bugs": {
    "url": "https://github.com/Neos21/ngx-markdown-wiki/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Neos21"
  },
  "dependencies": {
    "@angular/animations": "6.0.6",
    "@angular/common": "6.0.6",
    "@angular/compiler": "6.0.6",
    "@angular/core": "6.0.6",
    "@angular/forms": "6.0.6",
    "@angular/http": "6.0.6",
    "@angular/platform-browser": "6.0.6",
    "@angular/platform-browser-dynamic": "6.0.6",
    "@angular/router": "6.0.6",
    "@neos21/neos-normalize": "1.0.12",
    "core-js": "2.5.7",
    "marked": "0.4.0",
    "rxjs": "6.2.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.8",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.6",
    "@angular/language-service": "6.0.6",
    "@types/node": "8.9.5",
    "codelyzer": "4.2.1",
    "cpx": "1.5.0",
    "gh-pages": "1.0.0",
    "rimraf": "2.6.2",
    "ts-node": "5.0.1",
